<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:stretchColumns="1"
  tools:context=".RemovePatientattendsAppointmentFragment">
  <TableRow>
  <TextView
    android:id="@+id/removePatientattendsAppointmentpatientIdLabel"
    android:textStyle="bold"
    android:background="#EEFFBB"
    android:text="PatientId:" />
  <EditText
    android:id="@+id/removePatientattendsAppointmentpatientIdField"
    android:layout_span="4" />
  </TableRow>
  <TableRow>
  <TextView
    android:id="@+id/removePatientattendsAppointmentappointmentIdLabel"
    android:textStyle="bold"
    android:background="#EEFFBB"
    android:text="AppointmentId:" />
  <EditText
    android:id="@+id/removePatientattendsAppointmentappointmentIdField"
    android:layout_span="4" />
  </TableRow>
 <TableRow>
  <TextView
    android:id="@+id/removePatientattendsAppointmentpatientIdSpinnerLabel"
    android:textStyle="bold"
    android:background="#EEFFBB"
    android:text="Patient:" />
<Spinner
  android:id="@+id/removePatientattendsAppointmentpatientIdSpinner"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content" 
  android:layout_span="4" />
 </TableRow>
 <TableRow>
  <TextView
    android:id="@+id/removePatientattendsAppointmentappointmentIdSpinnerLabel"
    android:textStyle="bold"
    android:background="#EEFFBB"
    android:text="Appointment:" />
<Spinner
  android:id="@+id/removePatientattendsAppointmentappointmentIdSpinner"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content" 
  android:layout_span="4" />
 </TableRow>
 <TableRow>
  <Button
    android:id="@+id/removePatientattendsAppointmentOK"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_column="1"
    android:background="#AAAAFF"
    android:text="OK"
    android:onClick="removePatientattendsAppointmentOK"
    />
  <Button
    android:id="@+id/removePatientattendsAppointmentCancel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_column="3"
    android:background="#FFAAAA"
    android:text="Cancel"
    android:onClick="removePatientattendsAppointmentCancel"
    />
 </TableRow>
</TableLayout>
